// You are using GCC
#include<iostream>
using namespace std;

class c {
    
};
int main(){
    int n1,n2;
    cin >> n1 >> n2;
    cout << n1 << " " << n2 << endl;
    if(n1 <= 0 || n2 <= 0){
        cout << "0" ;
        return 0;
    }
    int max;
    for(int i=1;i<=n2;i++){
        if(n1%i == 0 && n2%i == 0){
            max = i;
        }
    }
    cout << max ;
    
    return 0;
    
}
